@model  List<SaafiLogistics.Models.User>
<h1>@{ViewBag.title = "List of Users";}</h1>

@if (Model.Count == 0)
{
    <p>No Users yet!</p>

}

<table class=" table">
    <tr>
        <th>UserName</th>
        <th>Email </th>
        <th>Password</th>
        <th>Verify</th>

    </tr>
    @foreach (var users in Model)
    {
        <tr>
            <td>@users.UserName</td>
            <td>@users.Email</td>
            <td>@users.Password</td>
            <td>@users.Verify</td>


        </tr>


    }
</table>

<p><a asp-controller="User" asp-action="Add">Add Load</a></p>